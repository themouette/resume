    // register module routes
    new Experiences();
    new Skills();
    new Home();
    var router = new Backbone.Router({ });
    Backbone.on('app:navigate', _.bind(router.navigate, router));

    // initialize canvas
    var canvas = new Canvas();

    $(function () {
        canvas.setElement($('body')).render();
        Backbone.history.start();
    });
});

